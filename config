#!/bin/bash

export PACKAGE_GZCAAS_NAME="gzcaas"
export PACKAGE_GZCAAS_ORG="go-zoox"
export PACKAGE_GZCAAS_BIN_NAME="$PACKAGE_GZCAAS_NAME"
export PACKAGE_GZCAAS_VERSION=v1.0.1

export PACKAGE_GZCAAS_GIT_REPOSITORY="https://github.com/${PACKAGE_GZCAAS_ORG}/${PACKAGE_GZCAAS_NAME}"

export PACKAGE_GZCAAS_RELEASE_DOWNLOAD="${PACKAGE_GZCAAS_RELEASE_DOWNLOAD}"

if [ -z "$PACKAGE_GZCAAS_RELEASE_DOWNLOAD" ]; then
  export PACKAGE_GZCAAS_RELEASE_DOWNLOAD="${PACKAGE_GZCAAS_GIT_REPOSITORY}/releases/download"
else
  export PACKAGE_GZCAAS_GIT_REPOSITORY=""
fi
