#!/bin/bash

export PACKAGE_CORE_NAME="gzcaas"
export PACKAGE_CORE_ORG="go-zoox"
export PACKAGE_CORE_BIN_NAME="$PACKAGE_CORE_NAME"
export PACKAGE_CORE_VERSION=v1.0.1

export PACKAGE_CORE_GIT_REPOSITORY="https://github.com/${PACKAGE_CORE_ORG}/${PACKAGE_CORE_NAME}"

export PACKAGE_CORE_RELEASE_DOWNLOAD="${PACKAGE_CORE_RELEASE_DOWNLOAD}"

if [ -z "$PACKAGE_CORE_RELEASE_DOWNLOAD" ]; then
  export PACKAGE_CORE_RELEASE_DOWNLOAD="${PACKAGE_CORE_GIT_REPOSITORY}/releases/download"
else
  export PACKAGE_CORE_GIT_REPOSITORY=""
fi
